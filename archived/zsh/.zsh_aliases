# Weather
alias weather='curl wttr.in/\?3F'

# Flush Directory Service cache
alias dns-flush='dscacheutil -flushcache && sudo killall -HUP mDNSResponder'
alias dns-off='echo "Unsetting DNS" && networksetup -setdnsservers Wi-Fi empty && echo "Flushing" && dns-flush'
alias dns-on='echo "Setting DNS" && networksetup -setdnsservers Wi-Fi 1.1.1.2 1.0.0.2 && echo "Flushing" && dns-flush'
alias dns-google='echo "Setting DNS" && networksetup -setdnsservers Wi-Fi 8.8.8.8 8.8.4.4 && echo "Flushing" && dns-flush'
alias dns-cloudflare='echo "Setting DNS" && networksetup -setdnsservers Wi-Fi 1.1.1.1 1.0.0.1 && echo "Flushing" && dns-flush'

# Clean up node_modules, build, and dist directories
alias clean-js='fd -t d "^(node_modules|build|dist)$" -x rm -rf {}'

